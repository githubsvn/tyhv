<h1>Company list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Charter_capital</th>
            <th>Phone</th>
            <th>Fax</th>
            <th>Mst</th>
            <th>Email</th>
            <th>Status</th>
            <th>Created_at</th>
            <th>Updated_at</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('admin_company_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.chartercapital }}</td>
            <td>{{ entity.phone }}</td>
            <td>{{ entity.fax }}</td>
            <td>{{ entity.mst }}</td>
            <td>{{ entity.email }}</td>
            <td>{{ entity.status }}</td>
            <td>{% if entity.createdat %}{{ entity.createdat|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.updatedat %}{{ entity.updatedat|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('admin_company_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('admin_company_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('admin_company_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
