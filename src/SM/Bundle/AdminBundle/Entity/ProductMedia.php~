<?php

namespace SM\Bundle\AdminBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ProductMedia
 *
 * @ORM\Table(name="mtx_product_media")
 * @ORM\Entity(repositoryClass="SM\Bundle\AdminBundle\Repository\ProductMediaRepository")
 */
class ProductMedia
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @ORM\ManyToOne(targetEntity="Products", cascade={"persist"})
     */
    private $product;

    /**
     * @ORM\ManyToOne(targetEntity="Media", cascade={"persist"})
     */
    private $media;

}
